import random as r



#run venv\Scripts\activate in terminal before running code to run virtual environment


characters = "1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz!@#$%^&*()_+-=[]\;,./<>?{|}~`'"
passwordStore = []
passwordLength = 16
newPassword = ''

for i in range(passwordLength):
    character = characters[r.randint(0,91)]
    passwordStore.append(character)
    

for c in passwordStore:
    newPassword += c  


print(f"New passsword is: {newPassword}")
